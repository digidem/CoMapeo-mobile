    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "husky": "^8.0.0",
    "jest": "^29.2.1",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "0.73.10",
    "patch-package": "^6.5.0",
    "prettier": "^3.0.2",
